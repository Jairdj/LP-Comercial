     member = index.view(listaPassadaPorParâmetro);
    -->
@model IEnumerable<Associations.Models.Member>
@{
    ViewBag.Title = "Index";
}

<h1>Membros</h1>

<a href="/Members/Create" class="btn btn-default">Novo Membro</a>

<table class="table">
    <thead>
        <tr>
            <td>Id</td>
            <td>Nome</td>
            <td>Ações</td>
        </tr>
    </thead>
    <tbody>
        @foreach( var item in Model ) {
            <tr>
                <td>@item.id</td>
                <td>@item.name</td>
                <td>
                    <a href="/Members/Edit/@item.id" class="btn btn-primary">Editar</a>
                    <a href="/Members/Delete/@item.id" class="btn btn-danger">Delete</a>
                </td>
            </tr>
            }
    </tbody>
</table>